{
  "version": 1,
  "projects": {
    "front-client": {
      "projectType": "application",
      "root": "apps/front/client",
      "sourceRoot": "apps/front/client/src",
      "prefix": "seed-client",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/front/client",
            "index": "apps/front/client/src/index.html",
            "main": "apps/front/client/src/main.ts",
            "polyfills": "apps/front/client/src/polyfills.ts",
            "tsConfig": "apps/front/client/tsconfig.app.json",
            "aot": true,
            "assets": ["apps/front/client/src/favicon.ico", "apps/front/client/src/assets"],
            "styles": ["apps/front/client/src/styles/index.sass"],
            "stylePreprocessorOptions": {
              "includePaths": ["apps/front/client/web/src/styles"]
            },
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/front/client/src/environments/environment.ts",
                  "with": "apps/front/client/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ]
            }
          },
          "outputs": ["{options.outputPath}"]
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "front-client:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "front-client:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "front-client:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/front/client/tsconfig.app.json", "apps/front/client/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**", "!apps/front/client/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/front/client/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/apps/front/client"]
        }
      }
    },
    "e2e-client": {
      "root": "apps/e2e/client",
      "sourceRoot": "apps/e2e/client/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/e2e/client/cypress.json",
            "tsConfig": "apps/e2e/client/tsconfig.e2e.json",
            "devServerTarget": "front-client:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "front-client:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/e2e/client/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/e2e/client/**/*"]
          }
        }
      }
    },
